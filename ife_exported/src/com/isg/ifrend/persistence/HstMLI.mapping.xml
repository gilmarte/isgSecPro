<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.isg.ifrend.model.bean.HstMli" table="HST_MLI">
		<id name="hstMliID" column="HST_ML_ID_PK" type="integer">
			<generator class="increment" />
		</id>
		<property name="mliID" column="HST_ML_ID" type="string" />
		<property name="countryCode" column="HST_ML_COUNTRYCODE_FK" type="string" />
		<property name="functionCode" column="HST_ML_FUNCTIONCODE" type="string" />
		<property name="userFieldID" column="HST_ML_USERFIELD_ID_FK" type="integer" />
		<property name="responseCodeID" column="HST_ML_RESPONSECODE_ID_FK" type="integer" />
		<property name="desc" column="HST_ML_DESC" type="string" />
		<property name="messageTypeID" column="HST_ML_MESSAGETYPE_ID" type="integer" />
		<property name="message" column="HST_ML_MESSAGE" type="string" />
		<property name="commentTypeID" column="HST_ML_COMMENTTYPE_ID" type="integer" />
		<property name="comment" column="HST_ML_COMMENT" type="string" />
		<property name="statusID" column="HST_ML_STATUS_ID" type="integer" />
		<property name="actionID" column="HST_ML_ACTION_ID" type="integer" />
		<property name="creatorUserID" column="HST_ML_CREATOR_USER_ID_FK" type="string" />
		<property name="dateCreated" column="HST_ML_DATE_CREATED" type="java.sql.Timestamp" />
		<property name="approverUserID" column="HST_ML_APPROVER_USER_ID_FK" type="string" />
		<property name="dateApproved" column="HST_ML_DATE_APPROVED" type="java.sql.Timestamp" />
		<property name="lastModifierUserID" column="HST_ML_LASTMODIFIER_USER_ID_FK" type="string" />
		<property name="dateLastModified" column="HST_ML_DATE_LAST_MODIFIED" type="java.sql.Timestamp" />
	</class>
</hibernate-mapping>