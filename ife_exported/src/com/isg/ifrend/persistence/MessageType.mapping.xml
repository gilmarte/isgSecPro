<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.isg.ifrend.model.bean">
	<class name="MessageType" table="ref_messagetypes">
		<id name="message_id" column="mt_messagetype_id_pk" type="integer">
			<generator class="increment" />
		</id>

		<property name="message_desc" column="mt_messagetype_desc" type="string" />
		
	</class>
</hibernate-mapping>