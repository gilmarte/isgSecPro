<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.isg.ifrend.model.bean">
	<class name="Operator" table="ref_operators">
		<id name="operator_code" column="op_operator_code_pk" type="string">
			<generator class="assigned" />
		</id>

		<property name="operator_desc" column="op_operator_desc"
			type="string" />
	</class>
</hibernate-mapping>