<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.isg.ifrend.model.bean">
	<class name="UserField" table="ref_userfields">
		<id name="userFieldID" column="uf_userfield_id_pk" type="integer">
			<generator class="increment" />
		</id>

		<property name="userFieldDesc" column="uf_userfield_desc" type="string" />
	</class>
</hibernate-mapping>