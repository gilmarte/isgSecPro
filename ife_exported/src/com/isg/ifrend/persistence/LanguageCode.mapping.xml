<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.isg.ifrend.model.bean">
	<class name="LanguageCode" table="ref_languagecodes">
		<id name="lcLanguageCodeID" column="lc_languagecode_pk" type="string">
			<generator class="native" />
		</id>

		<property name="lcLanguageCodeDesc" column="lc_languagecode_desc"
			type="string" />
	</class>
</hibernate-mapping>