<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.isg.ifrend.model.bean">
	<class name="DateFormats" table="ref_dateformats">
		<id name="dateformat_id" column="df_dateformat_id_pk" type="integer">
			<generator class="increment" />
		</id>

		<property name="dateformat_desc" column="df_dateformat_desc"
			type="string" />
	</class>
</hibernate-mapping>