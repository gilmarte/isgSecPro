<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.isg.ifrend.model.bean.HstScript" table="HST_SCRIPTS">
		<id name="hstScriptID" column="HST_SC_ID_PK" type="integer">
			<generator class="increment" />
		</id>
		<property name="scriptID" column="HST_SC_ID" type="string" />
		<property name="countryCode" column="HST_SC_COUNTRYCODE_FK" type="string" />
		<property name="functionCode" column="HST_SC_FUNCTIONCODE_FK" type="string" />
		<property name="userFieldID" column="HST_SC_USERFIELD_ID_FK" type="integer" />
		<property name="priorityID" column="HST_SC_PRIORITY_ID_FK" type="integer" />
		<property name="desc" column="HST_SC_DESC" type="string" />
		<property name="messageTypeID" column="HST_SC_MESSAGETYPE_ID" type="integer" />
		<property name="message" column="HST_SC_MESSAGE" type="string" />
		<property name="commentTypeID" column="HST_SC_COMMENTTYPE_ID" type="integer" />
		<property name="comment" column="HST_SC_COMMENT" type="string" />
		<property name="statusID" column="HST_SC_STATUS_ID" type="integer" />
		<property name="actionID" column="HST_SC_ACTION_ID" type="integer" />
		<property name="creatorUserID" column="HST_SC_CREATOR_USER_ID_FK" type="string" />
		<property name="dateCreated" column="HST_SC_DATE_CREATED" type="java.sql.Timestamp" />
		<property name="approverUserID" column="HST_SC_APPROVER_USER_ID_FK" type="string" />
		<property name="dateApproved" column="HST_SC_DATE_APPROVED" type="java.sql.Timestamp" />
		<property name="lastModifierUserID" column="HST_SC_LASTMODIFIER_USER_ID_FK" type="string" />
		<property name="dateLastModified" column="HST_SC_DATE_LAST_MODIFIED" type="java.sql.Timestamp" />
	</class>
</hibernate-mapping>