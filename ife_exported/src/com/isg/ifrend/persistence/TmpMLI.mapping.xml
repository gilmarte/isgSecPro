<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.isg.ifrend.model.bean.TmpMli" table="TMP_MLI">
		<id name="mliID" column="TMP_ML_ID_PK" type="string">
			<generator class="assigned" />
		</id>
		<property name="countryCode" column="TMP_ML_COUNTRYCODE_FK" type="string" />
		<property name="functionCode" column="TMP_ML_FUNCTIONCODE" type="string" />
		<property name="userFieldID" column="TMP_ML_USERFIELD_ID_FK" type="integer" />
		<property name="responseCodeID" column="TMP_ML_RESPONSECODE_ID_FK" type="integer" />
		<property name="desc" column="TMP_ML_DESC" type="string" />
		<property name="messageTypeID" column="TMP_ML_MESSAGETYPE_ID" type="integer" />
		<property name="message" column="TMP_ML_MESSAGE" type="string" />
		<property name="commentTypeID" column="TMP_ML_COMMENTTYPE_ID" type="integer" />
		<property name="comment" column="TMP_ML_COMMENT" type="string" />
		<property name="statusID" column="TMP_ML_STATUS_ID" type="integer" />
		<property name="actionID" column="TMP_ML_ACTION_ID" type="integer" />
		<property name="creatorUserID" column="TMP_ML_CREATOR_USER_ID_FK" type="string" />
		<property name="dateCreated" column="TMP_ML_DATE_CREATED" type="java.sql.Timestamp" />
		<property name="approverUserID" column="TMP_ML_APPROVER_USER_ID_FK" type="string" />
		<property name="dateApproved" column="TMP_ML_DATE_APPROVED" type="java.sql.Timestamp" />
		<property name="lastModifierUserID" column="TMP_ML_LASTMODIFIER_USER_ID_FK" type="string" />
		<property name="dateLastModified" column="TMP_ML_DATE_LAST_MODIFIED" type="java.sql.Timestamp" />
	</class>
</hibernate-mapping>